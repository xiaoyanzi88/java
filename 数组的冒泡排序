import java.util.Date;
public class 数组 {
	public static void main(String [] args){
		int []a= {45,89,7,6,0};
		int t=a[0];
		for(int j=0; j<4; j++)
			for(int i=0; i<4-j; i++)
			   if(a[i+1]<a[i])
			  {
				t=a[i];
				a[i]=a[i+1];
				a[i+1]=t;
			  }
		System.out.println("由小到大排序输出数组：");
		for(int i=0; i<5; i++)
		{
			System.out.print(a[i]+" ");
		}
		System.out.println();
		Date x=new Date();
		System.out.println(x);
	}
}
